{
  "name": "naf-css",
  "version": "0.0.7",
  "description": "A lightweight, progressive, PostCSS, glorified reset thingy.",
  "scripts": {
    "lint": "npx stylelint \"src/**/*.pcss\"",
    "lint:fix": "npx stylelint \"src/**/*.pcss\" --fix",
    "build": "npm run lint && node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgrammatiko/naf-css.git"
  },
  "author": "Dimitris Grammatikogiannis <d.grammatiko@gmail.com> (dgrammatiko.online)",
  "license": "MIT",
  "devDependencies": {
    "cssnano": "4.1.10",
    "kleur": "4.1.3",
    "modern-css-reset": "^1.1.1",
    "postcss": "8.1.1",
    "postcss-cli": "8.0.0",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-properties": "10.0.0",
    "postcss-custom-selectors": "5.1.2",
    "postcss-discard-comments": "4.0.2",
    "postcss-easy-import": "3.0.0",
    "postcss-import": "12.0.1",
    "postcss-mixins": "7.0.1",
    "postcss-nested": "5.0.1",
    "postcss-nesting": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-normalize": "9.0.0",
    "postcss-syntax": "0.36.2",
    "stylelint": "13.7.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "^4.0.0",
    "gzip-size": "5.1.1"
  },
  "params": {
    "browserslist": [
      "last 1 version"
    ],
    "customProperties": "true",
    "colorFunction": "true",
    "customSelectors": "true",
    "sourcemaps": "false",
    "compress": "true"
  }
}
